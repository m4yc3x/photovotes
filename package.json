{
  "name": "kabuvotez",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "npm run typeorm -- migration:generate -d src/migration.config.ts src/migrations/InitialMigration",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/migration.config.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/migration.config.ts",
    "migration:refresh": "typeorm-ts-node-esm migration:revert -d src/migration.config.ts --all && typeorm-ts-node-esm migration:run -d src/migration.config.ts",
    "migration:create": "typeorm-ts-node-esm migration:create src/migrations/InitialMigration"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.5",
    "https-browserify": "^1.0.0",
    "lucide-react": "^0.447.0",
    "next": "14.2.14",
    "node-fetch": "^2.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18",
    "react-dom": "^18",
    "reflect-metadata": "^0.2.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typeorm": "^0.3.20",
    "url": "^0.11.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "daisyui": "^4.12.12",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "mysql2": "^3.11.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "node-fetch": "^2.7.0",
    "uuid": "^10.0.0"
  }
}
